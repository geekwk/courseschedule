    $(document).ready(function(){
        $("#datatable-test tbody td").click(function (){
            var aPos = oTable.fnGetPosition(this)
            var adata = oTable.fnGetData(aPos[0])
            console.log(adata)
            adata[ aPos[1] ] = 'clicked';
            this.innerHTML = 'clicked';
            oTable.fnDeleteRow(aPos[1])

        })
        var oTable = $('#datatable-test').dataTable();
    })

    function update(){
        $('#datatable-test').dataTable().fnSetColumnVis(1,false)
    }
    function update2(){
        $('#datatable-test').dataTable().fnSetColumnVis(1,true)
